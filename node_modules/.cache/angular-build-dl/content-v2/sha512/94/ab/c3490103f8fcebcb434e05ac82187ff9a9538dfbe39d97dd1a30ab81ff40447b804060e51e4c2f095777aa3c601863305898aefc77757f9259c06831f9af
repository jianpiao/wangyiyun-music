        key: "playFirstSong",
        value: function playFirstSong() {
          if (this.playlistDetail.tracks.length > 0) {
            var song = this.playlistDetail.tracks[0];
            var art = song.ar.map(function (item) {
              return item.name;
            });
            this.playSong(song.id, song.name, song.dt, art, song.al.picUrl);
          } else {
            this.notification.create('error', "\u9519\u8BEF\u8B66\u544A", '没有播放的歌曲');
          }
        } // 播放歌单

      }, {
        key: "handlePlay",
        value: function handlePlay(song) {
          this.service.player.songList = this.playlistDetail.tracks;

          if (this.playlistDetail.tracks.length > 0) {
            var art = song.ar.map(function (item) {
              return item.name;
            });
            this.playSong(song.id, song.name, song.dt, art, song.al.picUrl);
          }
        } // 播放歌曲

      }, {
        key: "playSong",
        value: function playSong(id, name, size, artist, picUrl) {
          var _this47 = this;

          // 获取播放连接
          this.service.song.getSongUrl(id).subscribe(function (song) {
            if (song.url) {
              _this47.service.player.song = {
                id: id,
                name: name,
                url: song.url,
                size: size,
                artist: artist,
                picUrl: picUrl
              };
              var list = _this47.service.player.songList;

              if (!list.find(function (e) {
                return e.id == id;
              })) {
                _this47.service.player.songList.push(_this47.service.player.song);
              } // 获取歌词


              _this47.service.song.getLyric(id).subscribe(function (lyric) {
                _this47.service.player.song.lyric = lyric;
              });
            } else {
              _this47.notification.create('error', "\u9519\u8BEF\u8B66\u544A", '付费歌曲，无法播放');
            }
          }); // 开始播放歌曲

          this.service.player.start = true;
        } // 跳转到歌曲详情页面

      }, {
        key: "toSong",
        value: function toSong(id) {
          console.log('歌曲详情', id);
          this.router.navigate(['song'], {
