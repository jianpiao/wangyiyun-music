            _this11.artistDesc = res;
            _this11.loading = false;
          });
        } // 切换导航

      }, {
        key: "changeNav",
        value: function changeNav() {
          var index = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : 0;
          this.selectedNav = index;
          var id = this.id;
          this.loading = true;
          this.router.navigate(['artist'], {
            queryParams: {
              id: id,
              index: index
            }
          });

          switch (index) {
            case 0:
              this.getArtists(id);
              break;

            case 1:
              this.getAllAlnum(id);
              break;

            case 2:
              this.getArtistMv(id);
              break;

            default:
              this.getArtistDesc(id);
              break;
          }
        } // 跳转到歌手页面

      }, {
        key: "toArtist",
        value: function toArtist(id) {
          console.log('id', id);
          this.router.navigate(['artist'], {
            queryParams: {
              id: id
            }
          });
        }
      }]);

      return ArtistComponent;
    }();

