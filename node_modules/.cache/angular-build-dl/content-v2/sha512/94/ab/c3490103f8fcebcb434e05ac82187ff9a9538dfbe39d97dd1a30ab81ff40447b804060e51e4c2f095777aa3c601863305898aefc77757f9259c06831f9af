        this.artist = {
          id: '',
          name: '',
          picUrl: '',
          alias: []
        };
      }

      _createClass(ArtistComponent, [{
        key: "ngOnInit",
        value: function ngOnInit() {
          this.getData();
        } // 获取数据

      }, {
        key: "getData",
        value: function getData() {
          var _this6 = this;

          this.activatedRouter.queryParams.subscribe(function (params) {
            var id = params.id;
            _this6.id = id;
            _this6.loading = true;

            _this6.getArtistSimi(id); // 默认获取艺人信息


            _this6.getArtists(id); // 选择选中的类型
            // this.changeNav(params.index && parseInt(params.index))


            _this6.selectedNav = params.index && parseInt(params.index) || 0;
          });
        } // 获取热门歌手

      }, {
        key: "getArtistSimi",
        value: function getArtistSimi(id) {
          var _this7 = this;

          this.service.artist.getToplistArtist().subscribe(function (res) {
            _this7.hotArtist = res.filter(function (item, index) {
              return index < 6;
            });
          });
        } // 获取歌手单曲 

      }, {
        key: "getArtists",
        value: function getArtists(id) {
          var _this8 = this;

          this.service.artist.getArtists(id).subscribe(function (res) {
            _this8.artist = res.artist;
            _this8.hotSongs = res.hotSongs;
            _this8.loading = false;
          });
        } // 获取所有专辑

      }, {
        key: "getAllAlnum",
        value: function getAllAlnum(id) {
          var _this9 = this;

          this.service.artist.getArtistAlbum({
            id: id,
            limit: 50,
            offset: 0
          }).subscribe(function (res) {
            _this9.hotAlbums = res.hotAlbums;
            _this9.loading = false;
          });
        } // 获取mv

      }, {
        key: "getArtistMv",
