    /*! @angular/router */
    "./node_modules/@angular/router/fesm2015/router.js");

    var HotSongsComponent =
    /*#__PURE__*/
    function () {
      function HotSongsComponent(service, notification, router) {
        _classCallCheck(this, HotSongsComponent);

        this.service = service;
        this.notification = notification;
        this.router = router;
        this.songs = [];
      }

      _createClass(HotSongsComponent, [{
        key: "ngOnInit",
        value: function ngOnInit() {} // 播放歌单

      }, {
        key: "handlePlay",
        value: function handlePlay(song) {
          this.service.player.songList = this.songs;

          if (this.songs.length > 0) {
            var art = song.ar.map(function (item) {
              return item.name;
            });
            this.playSong(song.id, song.name, song.dt, art, song.al.picUrl);
          }
        } // 播放歌曲

      }, {
        key: "playSong",
        value: function playSong(id, name, size, artist, picUrl) {
          var _this12 = this;

          // 获取播放连接
          this.service.song.getSongUrl(id).subscribe(function (song) {
            if (song.url) {
              _this12.service.player.song = {
                id: id,
                name: name,
                url: song.url,
                size: size,
                artist: artist,
                picUrl: picUrl
              };
              var list = _this12.service.player.songList;

              if (!list.find(function (e) {
                return e.id == id;
              })) {
                _this12.service.player.songList.push(_this12.service.player.song);
              } // 获取歌词


              _this12.service.song.getLyric(id).subscribe(function (lyric) {
                _this12.service.player.song.lyric = lyric;
              });
            } else {
              _this12.notification.create('error', "\u9519\u8BEF\u8B66\u544A", '付费歌曲，无法播放');
            }
          }); // 开始播放歌曲

          this.service.player.start = true;
        } // 跳转到歌曲详情

      }, {
        key: "toSong",
        value: function toSong(id) {
          this.router.navigate(['song'], {
            queryParams: {
              id: id
            }
          });
        } // 跳转到艺人详情

      }, {
        key: "toArtist",
        value: function toArtist(id) {
          this.router.navigate(['artist'], {
            queryParams: {
              id: id
            }
          });
        } // 跳转到专辑详情

      }, {
        key: "toAlbum",
        value: function toAlbum(id) {
          this.router.navigate(['album'], {
            queryParams: {
              id: id
            }
          });
        }
      }]);

      return HotSongsComponent;
    }();

    HotSongsComponent.ctorParameters = function () {
      return [{
        type: src_app_services_service_service__WEBPACK_IMPORTED_MODULE_2__["ServiceService"]
      }, {
        type: ng_zorro_antd__WEBPACK_IMPORTED_MODULE_3__["NzNotificationService"]
      }, {
        type: _angular_router__WEBPACK_IMPORTED_MODULE_4__["Router"]
      }];
    };

    tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"])()], HotSongsComponent.prototype, "songs", void 0);
    HotSongsComponent = tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Component"])({
