        this.playerService = playerService;
        this.service = service;
        this.router = router;
        this.notification = notification;
        this.showSheet = false;
        this.currentTime = 0;
      }

      _createClass(PlayerComponent, [{
        key: "ngOnInit",
        value: function ngOnInit() {
          this.audioEl = this.audio.nativeElement;
        } // 缓存成功可以播放的时候

      }, {
        key: "onCanplay",
        value: function onCanplay() {
          this.playerService.start ? this.play() : this.pause();
        } // 

      }, {
        key: "play",
        value: function play() {
          this.audioEl.play();
          this.playerService.start ? null : this.playerService.start = true;
        } // 

      }, {
        key: "pause",
        value: function pause() {
          this.playerService.start ? this.playerService.start = false : null;
          this.audioEl.pause();
        } // 暂停/播放

      }, {
        key: "onPlayState",
        value: function onPlayState(state) {
          console.log(this.audioEl.currentTime);

          if (this.service.player.song.url) {
            state == 'play' ? this.pause() : this.play();
          } else {
            this.notification.blank('警告', '暂时无法播放歌曲');
          }
        } // 上一首

      }, {
        key: "onLast",
        value: function onLast() {
          if (this.service.player.songList.length > 0) {
            this.changeCurrentSong(0);
          } else {
            this.notification.blank('警告', '无法切换，暂时无播放的歌曲');
          }
        } // 下一首

      }, {
        key: "onNext",
        value: function onNext() {
          if (this.service.player.songList.length > 0) {
            this.changeCurrentSong(1);
          } else {
            this.notification.blank('警告', '无法切换，暂时无播放的歌曲');
          }
        } // 

      }, {
        key: "onEnd",
        value: function onEnd() {
          this.onNext();
        } // 改变当前播放的歌曲

      }, {
        key: "changeCurrentSong",
        value: function changeCurrentSong(type) {
          var _this54 = this;

          var list = this.playerService.songList;
          var index = list.findIndex(function (e) {
            return _this54.playerService.song.id == e.id;
          });

          if (type == 0) {
            this.playerService.song = list[index - 1] ? list[index - 1] : list[list.length - 1];
          } else {
            this.playerService.song = list[index + 1] ? list[index + 1] : list[0];
          }

          this.setPlay(this.playerService.song.id);
        }
      }, {
        key: "setPlay",
        value: function setPlay(id) {
          var _this55 = this;

          // 获取播放连接
          this.service.song.getSongUrl(id).subscribe(function (song) {
            _this55.service.player.song.url = song.url;
            _this55.service.player.song.size = song.size; // 获取歌词

            _this55.service.song.getLyric(id).subscribe(function (lyric) {
              _this55.service.player.song.lyric = lyric;
            });
          });
          this.play();
        } // 显示歌单

      }, {
        key: "onShowSheet",
        value: function onShowSheet() {
          this.showSheet = !this.showSheet;
        } // 播放选中的歌曲

      }, {
        key: "playItem",
        value: function playItem(item) {
          this.playerService.song = item;
          this.setPlay(item.id);
        } // 跳转到歌曲详情

      }, {
        key: "toSongDetail",
        value: function toSongDetail(item) {
          console.log(item);

          if (this.showSheet) {
            this.showSheet = false;
          }

          this.router.navigate(['song'], {
            queryParams: {
              id: item.id
            }
          });
        }
      }]);

      return PlayerComponent;
    }();

    PlayerComponent.ctorParameters = function () {
      return [{
        type: src_app_services_player_player_service__WEBPACK_IMPORTED_MODULE_2__["PlayerService"]
      }, {
        type: src_app_services_service_service__WEBPACK_IMPORTED_MODULE_3__["ServiceService"]
      }, {
        type: _angular_router__WEBPACK_IMPORTED_MODULE_4__["Router"]
      }, {
        type: ng_zorro_antd__WEBPACK_IMPORTED_MODULE_5__["NzNotificationService"]
      }];
    };

    tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["ViewChild"])('audio', {
      static: true
    })], PlayerComponent.prototype, "audio", void 0);
    PlayerComponent = tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Component"])({
      selector: 'app-player',
      template: tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(
      /*! raw-loader!./player.component.html */
      "./node_modules/raw-loader/dist/cjs.js!./src/app/share/components/player/player.component.html")).default,
      styles: [tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(
