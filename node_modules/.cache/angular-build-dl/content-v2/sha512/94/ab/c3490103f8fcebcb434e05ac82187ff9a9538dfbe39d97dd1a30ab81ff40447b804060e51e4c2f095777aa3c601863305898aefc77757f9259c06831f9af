        }, {
          img: '../../../../assets/images/create.jpg',
          name: '网易云原创音乐榜',
          data: []
        }];
        this.loadingRank = true; // 榜单加载

        this.loadingPlaylist = true; // 歌单加载

        this.loadingAlbum = true; // 专辑加载
      }

      _createClass(HomeComponent, [{
        key: "ngOnInit",
        value: function ngOnInit() {
          this.getBanner();
          this.getHotTags();
          this.getPersonalSheetList();
          this.getArtistList();
          this.getAlbumNewest();
          this.getTopListDj();
          this.getTopPlaylist(0);
          this.getTopPlaylist(2);
          this.getTopPlaylist(3);
        } // 获取轮播图

      }, {
        key: "getBanner",
        value: function getBanner() {
          var _this13 = this;

          this.service.home.getBanners().subscribe(function (banners) {
            _this13.banners = banners;
          });
        } // 获取热门歌单标签

      }, {
        key: "getHotTags",
        value: function getHotTags() {
          var _this14 = this;

          this.service.sheet.getHotTags().subscribe(function (tags) {
            _this14.hotTags = tags.filter(function (e, i) {
              return i < 5;
            });
          });
        } // 获取推荐歌单

      }, {
        key: "getPersonalSheetList",
        value: function getPersonalSheetList() {
          var _this15 = this;

          this.service.sheet.getPersonalSheetList().subscribe(function (result) {
            _this15.personalized = result.filter(function (e, i) {
              return i < 8;
            });
            _this15.loadingPlaylist = false;
          });
        } // 获取歌单列表

      }, {
        key: "topPlaylist",
        value: function topPlaylist() {
          var _this16 = this;

          var cat = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : '全部';
          var limit = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 8;
          this.loadingPlaylist = true;
          this.service.sheet.topPlaylist({
            order: 'hot',
            limit: limit,
            cat: cat
          }).subscribe(function (res) {
            console.log("获取歌单列表:", res);
            res.playlists = res.playlists.map(function (e) {
              e.picUrl = e.coverImgUrl;
              return e;
            });
            _this16.personalized = res.playlists;
            _this16.loadingPlaylist = false;
          });
        } // 入驻歌手

      }, {
        key: "getArtistList",
        value: function getArtistList() {
          var _this17 = this;

          this.service.home.getArtistList().subscribe(function (artist) {
            console.log("artist:", artist);
            _this17.artistList = artist.filter(function (item, index) {
              return index < 5;
            });
          });
        } // 获取最新专辑

      }, {
        key: "getAlbumNewest",
        value: function getAlbumNewest() {
          var _this18 = this;

          this.service.album.getAlbumNewest().subscribe(function (albums) {
            _this18.albumNewest = albums.filter(function (item, index) {
              return index < 8;
            });
            _this18.loadingAlbum = false;
          });
        } // 电台 - 新晋电台榜/热门电台榜

      }, {
        key: "getTopListDj",
        value: function getTopListDj() {
          var _this19 = this;

          this.service.home.getTopListDj().subscribe(function (toplist) {
            _this19.toplistDj = toplist.filter(function (item, index) {
              return index < 5;
            });
          });
        } // 排行榜

      }, {
        key: "getTopPlaylist",
        value: function getTopPlaylist(idx) {
          var _this20 = this;

          this.service.home.getTopPlaylist({
            idx: idx
          }).subscribe(function (playlist) {
            playlist = playlist.filter(function (item, index) {
              return index < 10;
            });

            switch (idx) {
              case 0:
                _this20.playlistType[1].data = playlist;
                break;

              case 2:
                _this20.playlistType[2].data = playlist;
                break;

              default:
                _this20.playlistType[0].data = playlist;
                break;
            }

            _this20.loadingRank = false;
          });
        } // 播放单曲

      }, {
        key: "playSong",
        value: function playSong(item) {
          var temp = item;
          this.setSong(item.id, item.name, temp.dt, temp.ar[0].name, temp.al.picUrl);
        }
      }, {
        key: "setSong",
        value: function setSong(id, name, size, artist, picUrl) {
          var _this21 = this;

          // 获取播放连接
          this.service.song.getSongUrl(id).subscribe(function (song) {
            _this21.service.player.song = {
              id: id,
              name: name,
              url: song.url,
              size: size,
              artist: artist,
              picUrl: picUrl
            };
            var list = _this21.service.player.songList;

            if (!list.find(function (e) {
              return e.id == id;
            })) {
              _this21.service.player.songList.push(_this21.service.player.song);
            } // 获取歌词


            _this21.service.song.getLyric(id).subscribe(function (lyric) {
              _this21.service.player.song.lyric = lyric;
            });
          }); // 开始播放歌曲

          this.service.player.start = true;
        }
      }, {
        key: "onSelectChange",
        value: function onSelectChange(e) {} // 

      }, {
        key: "selectBannerItem",
        value: function selectBannerItem(item, index) {
          if (item.targetId != 0) {
            this.jumpPage('album', item.targetId);
          } else {
            window.open(item.url);
          }
        } // 播放歌单

      }, {
        key: "playSheet",
        value: function playSheet(item) {
          var _this22 = this;

          this.service.sheet.getSheetList(item.id).subscribe(function (res) {
            _this22.service.player.songList = [];
            res.forEach(function (e) {
              _this22.service.player.songList.push({
                id: e.id,
                name: e.name,
                url: '.',
                size: e.dt,
                artist: e.ar[0].name,
                picUrl: e.al.picUrl
              });
            });

            if (res.length > 0) {
              var _item = res[0];

              _this22.setSong(_item.id, _item.name, _item.dt, _item.ar[0].name, _item.al.picUrl);
            }
          });
        } // 播放专辑

      }, {
        key: "playAlbum",
        value: function playAlbum(item) {
          var _this23 = this;

          this.service.album.getAlbumList(item.id).subscribe(function (album) {
            console.log("album", album);
            _this23.service.player.songList = [];
            album.forEach(function (e) {
              _this23.service.player.songList.push({
                id: e.id,
                name: e.name,
                url: '.',
                size: e.dt,
                artist: e.ar[0].name,
                picUrl: e.al.picUrl
              });
            });

            if (album.length > 0) {
              var _item2 = album[0];

              _this23.setSong(_item2.id, _item2.name, _item2.dt, _item2.ar[0].name, _item2.al.picUrl);
            }
          });
        } // 跳转到播放列表

      }, {
        key: "toPlaylist",
        value: function toPlaylist(item) {
          this.jumpPage('playlist', item.id);
        } // 跳转到用户页面

      }, {
        key: "toUser",
        value: function toUser(item) {
          this.jumpPage('user', item.id);
        } // 跳转到专辑页面

      }, {
        key: "toAlbum",
        value: function toAlbum(item) {
          this.jumpPage('album', item.id);
        } // 跳转页面

      }, {
        key: "jumpPage",
        value: function jumpPage(router, id) {
          this.router.navigate([router], {
            queryParams: {
              id: id
            }
          });
        } // 跳转歌曲详情

      }, {
        key: "toSong",
        value: function toSong(item) {
          this.jumpPage('song', item.id);
        } // 跳转到艺人页面

      }, {
        key: "jumpAritst",
        value: function jumpAritst(item, index) {
          this.router.navigate(['artist'], {
            queryParams: {
              id: item.id,
              index: index
            }
          });
        } // 跳转到用户页面

      }, {
        key: "jumpUser",
        value: function jumpUser(item) {
          this.router.navigate(['user'], {
            queryParams: {
              id: item.accountId
            }
          });
        } // 跳转到歌单列表

      }, {
        key: "jumpPlaylist",
        value: function jumpPlaylist() {
          this.router.navigate(['navPlaylist'], {
            queryParams: {
              nav: 2
            }
          });
        } // 跳转到新碟上架页面

      }, {
        key: "jumpAlbum",
        value: function jumpAlbum() {
          this.router.navigate(['navAlbum'], {
            queryParams: {
              nav: 5
            }
          });
        } // 跳转到榜单页面

      }, {
        key: "jumpRank",
        value: function jumpRank() {
          this.router.navigate(['navRank'], {
            queryParams: {
              nav: 1
            }
          });
        } // 跳转到歌手页面

      }, {
        key: "jumpSinger",
        value: function jumpSinger() {
          this.router.navigate(['navSinger'], {
            queryParams: {
              nav: 4
            }
          });
        }
      }]);

      return HomeComponent;
    }();

