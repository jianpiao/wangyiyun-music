      function NavRankComponent(service, activatedRoute, router, notification) {
        _classCallCheck(this, NavRankComponent);

        this.service = service;
        this.activatedRoute = activatedRoute;
        this.router = router;
        this.notification = notification;
        this.allList = [];
        this.list = [];
        this.selectedSlider = 0;
        this.hotComments = []; // 热门评论

        this.comments = []; // 评论

        this.playlistDetail = {
          id: 0,
          tracks: [],
          creator: {
            userId: 0,
            avatarUrl: '',
            nickname: ''
          },
          coverImgUrl: '',
          description: '',
          name: '',
          shareCount: 0,
          commentCount: 0,
          playCount: 0,
          createTime: 0,
          subscribedCount: 0,
          tags: []
        };
        this.description = [];
        this.loading = true; // 加载框
      }

      _createClass(NavRankComponent, [{
        key: "ngOnInit",
        value: function ngOnInit() {
          var _this41 = this;

          // 获取歌单详情
          this.activatedRoute.queryParams.subscribe(function (params) {
            if (params.idx) {
              _this41.getPlaylistDetail(params.idx); // 获取榜单列表


              _this41.getAllTopPlaylist();
            } else {
              // 获取榜单列表
              _this41.getAllTopPlaylist(1);
            }
          });
        } // 排行榜

      }, {
        key: "getTopPlaylist",
        value: function getTopPlaylist(idx) {
          var _this42 = this;

          this.service.home.getTopPlaylist({
            idx: idx
          }).subscribe(function (playlist) {
            console.log("获取排行榜到数据：", playlist);
            _this42.list = playlist;
          });
        } // 获取所有榜单

      }, {
        key: "getAllTopPlaylist",
        value: function getAllTopPlaylist() {
          var _this43 = this;

          var status = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : 0;
          this.service.home.getAllTopPlaylist().subscribe(function (res) {
            _this43.allList = res; // 如果url地址的id不存在则区榜单列表第一个的id

            if (status == 1) {
              _this43.getPlaylistDetail(res[0] && res[0].id);
            }
          });
        } // 获取歌单列表详情

      }, {
        key: "getPlaylistDetail",
        value: function getPlaylistDetail(id) {
          var _this44 = this;

          this.loading = true;
          this.service.sheet.getSheetListDetail(id).subscribe(function (playlist) {
            _this44.playlistDetail = playlist;

            if (playlist.description) {
              _this44.description = playlist.description.split("\n");
            }
          }); // 获取评论

          this.getPlaylistComments(id);
        } // 获取歌单评论

      }, {
        key: "getPlaylistComments",
        value: function getPlaylistComments(id, offset, before) {
          var _this45 = this;

          this.service.sheet.getSheetListComments({
            id: id,
            offset: offset,
            limit: 20,
            before: before
          }).subscribe(function (res) {
            _this45.commentTotal = res.total;
            _this45.comments = res.comments;

            if (_this45.hotComments.length == 0) {
              _this45.hotComments = res.hotComments;
            }

            _this45.loading = false;
          });
        } // 选中侧栏

