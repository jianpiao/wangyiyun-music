      function NavPlaylistComponent(service, router) {
        _classCallCheck(this, NavPlaylistComponent);

        this.service = service;
        this.router = router;
        this.playlists = [];
        this.selectedTag = '全部';
        this.currentPage = 1;
        this.total = 0;
        this.tags = {
          all: {},
          sub: [],
          categories: []
        };
        this.loading = false; // 加载状态
      }

      _createClass(NavPlaylistComponent, [{
        key: "ngOnInit",
        value: function ngOnInit() {
          this.playlistCatlist();
          this.topPlaylist();
        } // 获取热门歌单标签

      }, {
        key: "getHotTags",
        value: function getHotTags() {
          this.service.sheet.getHotTags().subscribe(function (tags) {
            console.log("热门歌单标签：", tags);
          });
        }
      }, {
        key: "playlistCatlist",
        value: function playlistCatlist() {
          var _this39 = this;

          this.service.sheet.playlistCatlist().subscribe(function (res) {
            console.log("歌单分类", res);
            res.categories = Object.values(res.categories).map(function (item) {
              return item;
            });
            _this39.tags = res;
          });
        } // 获取最新数据

      }, {
        key: "topPlaylist",
        value: function topPlaylist() {
          var _this40 = this;

          var cat = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : '全部';
          var limit = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 50;
          this.loading = true;
          this.service.sheet.topPlaylist({
            order: 'hot',
            limit: limit,
            cat: cat
          }).subscribe(function (res) {
            console.log("获取最新歌单列表:", res);
            _this40.total = res.total;
            _this40.playlists = res.playlists;
            _this40.loading = false;
          });
        } // 选择歌单类型

      }, {
        key: "selectTag",
        value: function selectTag(item) {
          this.selectedTag = item.name;
          this.topPlaylist(item.name);
        } // 跳转到播放列表

      }, {
        key: "toPlaylist",
        value: function toPlaylist(item) {
          this.router.navigate(['playlist'], {
            queryParams: {
              id: item.id
            }
          });
        } // 分页

      }, {
        key: "nzPageIndexChange",
        value: function nzPageIndexChange(page) {
          console.log('page:', page);
          this.topPlaylist(this.selectedTag, page * 50);
        }
      }]);

      return NavPlaylistComponent;
    }();

