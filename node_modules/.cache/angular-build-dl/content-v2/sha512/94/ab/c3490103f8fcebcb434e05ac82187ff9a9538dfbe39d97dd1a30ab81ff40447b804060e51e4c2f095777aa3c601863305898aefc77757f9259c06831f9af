  /*! exports provided: AlbumComponent */

  /***/
  function srcAppPagesComponentsAlbumAlbumAlbumComponentTs(module, __webpack_exports__, __webpack_require__) {
    "use strict";

    __webpack_require__.r(__webpack_exports__);
    /* harmony export (binding) */


    __webpack_require__.d(__webpack_exports__, "AlbumComponent", function () {
      return AlbumComponent;
    });
    /* harmony import */


    var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(
    /*! tslib */
    "./node_modules/tslib/tslib.es6.js");
    /* harmony import */


    var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(
    /*! @angular/core */
    "./node_modules/@angular/core/fesm2015/core.js");
    /* harmony import */


    var src_app_services_service_service__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(
    /*! src/app/services/service.service */
    "./src/app/services/service.service.ts");
    /* harmony import */


    var _angular_router__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(
    /*! @angular/router */
    "./node_modules/@angular/router/fesm2015/router.js");

    var AlbumComponent =
    /*#__PURE__*/
    function () {
      function AlbumComponent(service, activatedRoute, router) {
        _classCallCheck(this, AlbumComponent);

        this.service = service;
        this.activatedRoute = activatedRoute;
        this.router = router;
        this.albumDetail = {
          id: 0,
          songs: [],
          album: {
            artist: {
              id: 0,
              name: '',
              picUrl: ''
            },
            company: '',
            name: '',
            size: 0,
            description: '',
            publishTime: 0,
            picUrl: '',
            info: {
              commentCount: 0,
              shareCount: 0
            }
          }
        };
        this.description = []; // 简介内容

        this.showDescription = false; // 是否展开全部介绍

        this.hotComments = []; // 热门评论

        this.comments = []; // 评论
      }

      _createClass(AlbumComponent, [{
        key: "ngOnInit",
        value: function ngOnInit() {
          this.getAlbumDetail();
        } // 获取专辑详情

      }, {
        key: "getAlbumDetail",
        value: function getAlbumDetail() {
          var _this = this;

          this.activatedRoute.queryParams.subscribe(function (params) {
            // 专辑详情
            _this.service.album.getAlbumDetail(params.id).subscribe(function (album) {
              console.log("album", album);

              if (album.songs.length > 0) {
                var al = JSON.parse(JSON.stringify(album)); // 热门专辑

                _this.getAllAlnum(al.songs[0].ar[0].id);

                album.songs = album.songs.map(function (item) {
                  return {
                    id: item.id,
                    name: item.name,
                    url: '.',
                    size: item.dt,
                    artist: item.ar[0].name,
                    picUrl: item.al.picUrl
                  };
                });
              }

              _this.albumDetail = album;
              _this.description = album.album.description.split('\n');
            }); // 获取评论


            _this.getAlbumComments(params.id);
          });
        } // 获取热门专辑

      }, {
        key: "getAllAlnum",
        value: function getAllAlnum(id) {
          var _this2 = this;

          this.service.artist.getArtistAlbum({
            id: id,
            limit: 50,
            offset: 0
          }).subscribe(function (res) {
            _this2.hotAlbums = res.hotAlbums.filter(function (item, index) {
              return index < 10;
            });
          });
        } // 获取专辑评论

      }, {
        key: "getAlbumComments",
        value: function getAlbumComments(id) {
          var _this3 = this;

          var offset = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 0;
          var before = arguments.length > 2 ? arguments[2] : undefined;
          this.service.album.getAlbumComments({
            id: id,
            offset: offset * 20,
            limit: 20,
            before: before
          }).subscribe(function (res) {
            console.log("评论列表", res);
            _this3.commentTotal = res.total;
            _this3.comments = res.comments;

            if (_this3.hotComments.length == 0) {
              _this3.hotComments = res.hotComments;
            }
          });
        } // 评论分页

      }, {
        key: "pageIndexChange",
        value: function pageIndexChange(page) {
          var _this4 = this;

          this.activatedRoute.queryParams.subscribe(function (params) {
            _this4.getAlbumComments(params.id, page - 1);
          });
        } // 播放歌单

      }, {
        key: "handlePlay",
        value: function handlePlay(song) {
          this.service.player.songList = this.albumDetail.songs;

          if (this.albumDetail.songs.length > 0) {
            this.playSong(song.id, song.name, song.size, song.artist, song.picUrl);
          }
        } // 播放歌曲

      }, {
        key: "playSong",
        value: function playSong(id, name, size, artist, picUrl) {
          var _this5 = this;

          // 获取播放连接
          this.service.song.getSongUrl(id).subscribe(function (song) {
            _this5.service.player.song = {
              id: id,
              name: name,
              url: song.url,
              size: size,
              artist: artist,
              picUrl: picUrl
            };
            var list = _this5.service.player.songList;

            if (!list.find(function (e) {
              return e.id == id;
            })) {
              _this5.service.player.songList.push(_this5.service.player.song);
            } // 获取歌词


            _this5.service.song.getLyric(id).subscribe(function (lyric) {
              _this5.service.player.song.lyric = lyric;
            });
          }); // 开始播放歌曲

          this.service.player.start = true;
        } // 播放第一首歌

      }, {
        key: "playFirstSong",
        value: function playFirstSong() {
          var song = this.albumDetail.songs[0];
          this.playSong(song.id, song.name, song.size, song.artist, song.picUrl);
        } // 跳转到歌手页面

      }, {
        key: "toArtist",
        value: function toArtist(id) {
          var index = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 0;
          this.router.navigate(['/artist'], {
            queryParams: {
              id: id,
              index: index
            }
