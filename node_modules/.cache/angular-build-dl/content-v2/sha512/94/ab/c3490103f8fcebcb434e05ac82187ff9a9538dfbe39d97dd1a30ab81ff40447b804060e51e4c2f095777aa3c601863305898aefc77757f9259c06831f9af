    function () {
      function PlaylistComponent(service, activatedRoute, router, notification) {
        _classCallCheck(this, PlaylistComponent);

        this.service = service;
        this.activatedRoute = activatedRoute;
        this.router = router;
        this.notification = notification;
        this.playlistDetail = {
          id: 0,
          tracks: [],
          creator: {
            userId: 0,
            avatarUrl: '',
            nickname: ''
          },
          coverImgUrl: '',
          description: '',
          name: '',
          shareCount: 0,
          commentCount: 0,
          playCount: 0,
          createTime: 0,
          subscribedCount: 0,
          tags: []
        };
        this.hotComments = []; // 热门评论

        this.comments = []; // 评论

        this.description = [];
      }

      _createClass(PlaylistComponent, [{
        key: "ngOnInit",
        value: function ngOnInit() {
          this.getPlaylistDetail();
          this.getHotPlaylist();
        } // 获取歌单列表详情

      }, {
        key: "getPlaylistDetail",
        value: function getPlaylistDetail() {
          var _this24 = this;

          this.activatedRoute.queryParams.subscribe(function (params) {
            _this24.service.sheet.getSheetListDetail(params.id).subscribe(function (playlist) {
              _this24.playlistDetail = playlist;

              if (playlist.description) {
                _this24.description = playlist.description.split("\n");
              }
            }); // 获取评论


            _this24.getPlaylistComments(params.id);
          });
        } // 获取歌单评论

      }, {
        key: "getPlaylistComments",
        value: function getPlaylistComments(id, offset, before) {
          var _this25 = this;

          this.service.sheet.getSheetListComments({
            id: id,
            offset: offset,
            limit: 20,
            before: before
          }).subscribe(function (res) {
            _this25.commentTotal = res.total;
            _this25.comments = res.comments;

            if (_this25.hotComments.length == 0) {
              _this25.hotComments = res.hotComments;
            }
          });
        } // 获取热门歌单

      }, {
        key: "getHotPlaylist",
        value: function getHotPlaylist() {
          var _this26 = this;

          this.service.sheet.getPersonalSheetList().subscribe(function (result) {
            _this26.personalized = result.filter(function (e, i) {
              return i < 5;
            });
          });
        } // 评论分页

      }, {
        key: "pageIndexChange",
        value: function pageIndexChange(page) {
          var _this27 = this;

          this.activatedRoute.queryParams.subscribe(function (params) {
            _this27.getPlaylistComments(params.id, page - 1);
          });
        } // 跳转到个人用户

      }, {
        key: "jumpUser",
        value: function jumpUser(id) {
          this.router.navigate(['user'], {
            queryParams: {
              id: id
            }
          });
        } // 跳转到推荐歌单

      }, {
        key: "toPlaylist",
        value: function toPlaylist(id) {
          this.router.navigate(['playlist'], {
            queryParams: {
              id: id
            }
          });
        } // 播放第一首歌

      }, {
        key: "playFirstSong",
        value: function playFirstSong() {
          if (this.playlistDetail.tracks.length > 0) {
            var song = this.playlistDetail.tracks[0];
            var art = song.ar.map(function (item) {
              return item.name;
            });
            this.playSong(song.id, song.name, song.dt, art, song.al.picUrl);
          } else {
            this.notification.create('error', "\u9519\u8BEF\u8B66\u544A", '没有播放的歌曲');
          }
        } // 播放歌单

      }, {
        key: "handlePlay",
        value: function handlePlay(song) {
          this.service.player.songList = this.playlistDetail.tracks;

          if (this.playlistDetail.tracks.length > 0) {
            var art = song.ar.map(function (item) {
              return item.name;
            });
            this.playSong(song.id, song.name, song.dt, art, song.al.picUrl);
          }
        } // 播放歌曲

      }, {
        key: "playSong",
        value: function playSong(id, name, size, artist, picUrl) {
          var _this28 = this;

          // 获取播放连接
          this.service.song.getSongUrl(id).subscribe(function (song) {
            if (song.url) {
              _this28.service.player.song = {
                id: id,
                name: name,
                url: song.url,
                size: size,
                artist: artist,
                picUrl: picUrl
              };
              var list = _this28.service.player.songList;

              if (!list.find(function (e) {
                return e.id == id;
              })) {
                _this28.service.player.songList.push(_this28.service.player.song);
              } // 获取歌词


              _this28.service.song.getLyric(id).subscribe(function (lyric) {
                _this28.service.player.song.lyric = lyric;
              });
            } else {
              _this28.notification.create('error', "\u9519\u8BEF\u8B66\u544A", '付费歌曲，无法播放');
            }
          }); // 开始播放歌曲

          this.service.player.start = true;
        } // 跳转到歌曲详情页面

      }, {
        key: "toSong",
        value: function toSong(id) {
          console.log('歌曲详情', id);
          this.router.navigate(['song'], {
            queryParams: {
              id: id
            }
          });
        } // 跳转到歌手页面

      }, {
        key: "toArtist",
        value: function toArtist(id) {
          console.log('歌手页面', id);
          this.router.navigate(['artist'], {
            queryParams: {
              id: id
            }
          });
        } // 跳转到专辑页面

      }, {
        key: "toAlbum",
        value: function toAlbum(id) {
          console.log("专辑页面:", id);
          this.router.navigate(['album'], {
            queryParams: {
              id: id
            }
          });
        } // 收藏

      }, {
        key: "favorite",
        value: function favorite() {
          this.notification.create('success', "\u63D0\u793A", '收藏功能，等待开发');
        } // 分享

      }, {
        key: "share",
        value: function share() {
          this.notification.create('success', "\u63D0\u793A", '分享功能，等待开发');
        } // 发私信

